#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xfcdc30 .scope module, "multiplexer_testbench" "multiplexer_testbench" 2 1;
 .timescale 0 0;
v0xfe0690_0 .var "address", 4 0;
v0xfe0760_0 .var "inputs", 31 0;
v0xfe0810_0 .net "out", 0 0, L_0xfe08c0; 1 drivers
S_0xfcdd20 .scope module, "r" "mux32to1by1" 2 5, 3 1, S_0xfcdc30;
 .timescale 0 0;
v0xfa9de0_0 .net "address", 4 0, v0xfe0690_0; 1 drivers
v0xfe0550_0 .net "inputs", 31 0, v0xfe0760_0; 1 drivers
v0xfe05f0_0 .alias "out", 0 0, v0xfe0810_0;
L_0xfe08c0 .part/v v0xfe0760_0, v0xfe0690_0, 1;
    .scope S_0xfcdc30;
T_0 ;
    %vpi_call 2 8 "$display", " ";
    %vpi_call 2 9 "$display", "  addr   inputs                            | out | expected out";
    %set/v v0xfe0690_0, 0, 5;
    %movi 8, 1, 32;
    %set/v v0xfe0760_0, 8, 32;
    %delay 1000, 0;
    %vpi_call 2 11 "$display", "  %b  %b  |  %b  |  %b", v0xfe0690_0, v0xfe0760_0, v0xfe0810_0, 1'b1;
    %movi 8, 4, 5;
    %set/v v0xfe0690_0, 8, 5;
    %movi 8, 1, 32;
    %set/v v0xfe0760_0, 8, 32;
    %delay 1000, 0;
    %vpi_call 2 14 "$display", "  %b  %b  |  %b  |  %b", v0xfe0690_0, v0xfe0760_0, v0xfe0810_0, 1'b0;
    %movi 8, 1, 5;
    %set/v v0xfe0690_0, 8, 5;
    %movi 8, 2, 32;
    %set/v v0xfe0760_0, 8, 32;
    %delay 1000, 0;
    %vpi_call 2 17 "$display", "  %b  %b  |  %b  |  %b", v0xfe0690_0, v0xfe0760_0, v0xfe0810_0, 1'b1;
    %movi 8, 4, 5;
    %set/v v0xfe0690_0, 8, 5;
    %movi 8, 2, 32;
    %set/v v0xfe0760_0, 8, 32;
    %delay 1000, 0;
    %vpi_call 2 20 "$display", "  %b  %b  |  %b  |  %b", v0xfe0690_0, v0xfe0760_0, v0xfe0810_0, 1'b0;
    %vpi_call 2 22 "$display", " ";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "verilog/multiplexer.t.v";
    "verilog/multiplexer.v";
